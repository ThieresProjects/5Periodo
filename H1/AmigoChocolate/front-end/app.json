{
  "expo": {
    "name": "Amigo Chocolate",
    "slug": "Amigo Chocolate",
    "version": "1.0.0",
    "scheme": "amchoco",
    "orientation": "portrait",
    "icon": "./src/Content/Assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/Content/Images/logo/splash-choco.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/Content/Assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.amchoco"
    },
    "web": {
      "favicon": "./src/Content/Assets/favicon.png"
    }
  }
}
